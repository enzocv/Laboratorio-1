      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select emp.last_name AS Empleado, emp.salary AS 'Salaio Mensual'
	FROM employees emp inner join departments dep
	on (emp.salary between 5000 and 12000) and (dep.department_id in(20,50));</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
