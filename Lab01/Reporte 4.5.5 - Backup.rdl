      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        LEFT(last_name, 1) + LOWER(RIGHT(last_name, LEN(last_name) - 1)) AS 'Apellidos', LEN(last_name) AS Longitud
FROM            employees
WHERE        (last_name LIKE 'J%')
ORDER BY last_name</CommandText>
        <Timeout>0</Timeout>
      </Query>
