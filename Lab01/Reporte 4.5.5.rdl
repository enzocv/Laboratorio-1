  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Apellido_letra">
            <Value>=Parameters!Apellido_letra.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        LEFT(last_name, 1) + LOWER(RIGHT(last_name, LEN(last_name) - 1)) AS 'Apellidos', LEN(last_name) AS Longitud
FROM            employees
WHERE        (last_name LIKE @Apellido_letra + '%')
ORDER BY last_name</CommandText>
      </Query>
      <Fields>
        <Field Name="Apellidos">
          <DataField>Apellidos</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Longitud">
