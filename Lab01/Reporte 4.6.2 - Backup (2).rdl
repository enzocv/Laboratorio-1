        <Field Name="Fecha_de_Contratacion">
          <DataField>Fecha de Contratacion</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Fecha_de_Revision">
          <DataField>Fecha de Revision</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select last_name as Apellido,hire_date as "Fecha de Contratacion",datename(dw,DATEADD(MONTH,6,hire_date)) 
						   + ','+ convert(varchar(50), datepart(day,DATEADD(month,6,hire_date)))
						   +' de '+datename(month,dateadd(month,6,hire_date))
						   +','+ convert(varchar(50), datepart(year,dateadd(month,6,hire_date))) as "Fecha de Revision"
from employees</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
