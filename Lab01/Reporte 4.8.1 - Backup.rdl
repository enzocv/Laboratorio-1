      </Fields>
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        loc.location_id AS 'ID de la Ubicacion', loc.street_address AS 'Direccion', loc.city AS 'cuidad', loc.state_province AS 'provincia', countries.country_name AS 'pais'
FROM            locations AS loc INNER JOIN
                         countries ON loc.country_id = countries.country_id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
